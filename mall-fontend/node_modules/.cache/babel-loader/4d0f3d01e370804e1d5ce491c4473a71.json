gister,\n            meta: {show: false}\n        },\n        {\n            path: \"/search\",\n            component: Search,\n            meta: {show: true}\n        },\n        {\n            // 重定向到主页\n            path: \"/\",\n            redirect: \"/home\"\n        }\n    ]\n})\n"]}]}